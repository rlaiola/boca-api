{
  "name": "boca-api",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "git@github.com:matheuslenke/Boca-API.git",
  "author": "Matheus Lenke <matheuslenke@gmail.com>",
  "contributors": [
    {
      "name": "Leonardo Deorce",
      "email": "leodeorce@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --transpile-only src/server.ts",
    "docker:up": "docker-compose -f docker-compose.yml -f docker-compose-api.yml up -d",
    "test:docker:up": "docker-compose -f docker-compose.yml -f docker-compose-test.yml up -d",
    "docker:apilogs": "docker logs boca_api -f",
    "docker:postgreslogs": "docker logs boca-postgres -f",
    "docker:down": "docker-compose -f docker-compose.yml -f docker-compose-api.yml down",
    "test:docker:down": "docker-compose -f docker-compose.yml -f docker-compose-test.yml down -v",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "lint": "eslint --fix --ext .ts ."
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "pg": "^8.7.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2"
  }
}